{
  "name": "bell24H-dashboard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18 <=22"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start -p ${PORT:-3000}",
    "prisma:generate": "prisma generate",
    "start:prod": "node server.js",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "migrate:prod": "npx prisma migrate deploy",
    "migrate:reset": "npx prisma migrate reset --force",
    "db:seed": "node prisma/seed.js",
    "seed:admin": "node prisma/seed-admin.js",
    "deploy:safe": "node scripts/deploy-safe.cjs",
    "backup": "node scripts/backup.cjs",
    "protect": "node scripts/protect-files.cjs",
    "verify": "node scripts/protect-files.cjs verify",
    "predeploy": "npm run verify && npm run backup",
    "deploy:staging": "npm run predeploy && vercel --env preview",
    "deploy:production": "node scripts/deploy-production.js",
    "audit": "node scripts/audit-pages.cjs",
    "audit:visual": "open admin/audit-dashboard.html",
    "migrate:missing": "node scripts/migrate-missing-pages.cjs",
    "migrate:test": "node scripts/migrate-missing-pages.cjs --dry-run",
    "compare": "npm run audit && npm run audit:visual",
    "recovery:pull": "node scripts/pull-vercel-source.cjs",
    "recovery:verify": "node scripts/verify-pages.cjs",
    "recovery:test": "node scripts/test-all-pages.cjs",
    "recovery:full": "npm run recovery:pull && npm run recovery:verify && npm run recovery:test",
    "recovery:backup": "node scripts/backup-current.cjs",
    "workflow:pre": "node scripts/dev-workflow.cjs pre",
    "workflow:post": "node scripts/dev-workflow.cjs post",
    "workflow:branch": "node scripts/dev-workflow.cjs branch",
    "workflow:staging": "node scripts/dev-workflow.cjs staging",
    "workflow:production": "node scripts/dev-workflow.cjs production",
    "workflow:show": "node scripts/dev-workflow.cjs show",
    "env:setup": "node scripts/setup-env.cjs create",
    "env:validate": "node scripts/setup-env.cjs validate",
    "env:status": "node scripts/setup-env.cjs status",
    "api:setup": "node scripts/setup-api-integrations.cjs",
    "api:test": "node scripts/test-api-integrations.cjs",
    "db:setup": "node scripts/setup-database.cjs setup",
    "db:status": "node scripts/setup-database.cjs status",
    "audit:admin": "node scripts/audit-admin-pages.js",
    "backup:vercel": "node scripts/backup-vercel-admin.js backup",
    "backup:list": "node scripts/backup-vercel-admin.js list",
    "backup:restore": "node scripts/backup-vercel-admin.js restore",
    "merge:admin": "node scripts/smart-admin-merge.js",
    "merge:safe": "npm run backup:vercel && npm run audit:admin && npm run merge:admin",
    "deploy:admin": "node scripts/deploy-admin-safely.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ci": "playwright install --with-deps && playwright test --reporter=line",
    "test:db-load": "node scripts/dbLoadTest.js",
    "deploy:vercel": "node scripts/deploy-vercel.js",
    "load-test": "node scripts/load-test.js",
    "test:auth": "curl -X POST http://localhost:3000/api/auth/send-phone-otp -H 'Content-Type: application/json' -d '{\"phone\": \"+919876543210\"}'",
    "test:payment": "curl -X POST http://localhost:3000/api/payments/create-order -H 'Content-Type: application/json' -d '{\"amount\": 1000, \"currency\": \"INR\"}'",
    "health-check": "node scripts/health-check.js",
    "deploy:vercel": "deploy-to-vercel.bat"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@chakra-ui/react": "^2.9.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.1",
    "@openzeppelin/contracts": "^4.9.5",
