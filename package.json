    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "formidable": "^3.5.4",
    "framer-motion": "^10.10.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "multer": "^2.0.1",
    "next": "^14.2.3",
    "next-auth": "^4.24.11",
    "next-cloudinary": "^6.16.0",
    "openai": "^4.24.0",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "prisma": "^6.15.0",
    "qrcode.react": "^4.2.0",
    "razorpay": "^2.9.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-redux": "^9.2.0",
    "resend": "^6.0.3",
    "sharp": "^0.34.2",
    "stripe": "^18.3.0",
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1",
    "ua-parser-js": "^2.0.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@faker-js/faker": "^9.3.1",
    "@nomicfoundation/hardhat-toolbox": "^6.0.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/pdfkit": "^0.13.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.3.3",
    "eslint-plugin-security": "^3.0.1",
    "hardhat": "^2.25.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0",
    "msw": "^2.10.2",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.6",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}